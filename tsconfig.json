{
  "compilerOptions": {
    "target": "es5",
    "module": "system", /* 'none', 'commonjs', 'amd', 'system', 'umd', 'es6', or 'es2015' */
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
